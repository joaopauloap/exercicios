programa {
	funcao inicio() {
        real a,b,maior
        
        escreva("digite valor de a: ")
        leia(a)
        
        escreva("digite valor de b: ")
        leia(b)
        
        se(a>b){
            maior = a
        }senao{
            maior = b
        }
        
        escreva("O maior valor é: ",maior)
        
	}
}
